    ### ###########################################
    ### webern-person:E39_Actor

    webern-person:E39_Actor rdf:type owl:Class ;

            kb:resourceIcon "default.png" ;

            prov:wasDerivedFrom ecrm:E39_Actor ;

            rdfs:comment    """Diese Klasse umfasst Personen, entweder einzeln oder in Gruppen, die das Potenzial haben, absichtliche Handlungen durchzuführen, für die jemand verantwortlich gemacht werden kann."""@de ,
                            """This class comprises people, either individually or in groups, who have the potential to perform intentional actions of kinds for which someone may be held responsible."""@en ,
                            """Cette classe comprend les personnes, individuellement ou en groupe, qui ont le potentiel d'accomplir des actions intentionnelles dont quelqu'un peut être tenu responsable."""@fr ,
                            """Questa classe comprende persone, sia individualmente che in gruppo, che hanno il potenziale per compiere azioni intenzionali di tipo di cui qualcuno può essere ritenuto responsabile."""@it ;

            rdfs:isDefinedBy <http://www.knora.org/ontology/08AE/webern-person> ;

            rdfs:label  "E39 Akteur"@de ,
                        "E39 Actor"@en ,
                        "E39 Acteur"@fr ,
                        "E39 Attore"@it ;

            rdfs:subClassOf webern-works:E77_PersistentItem , ecrm:E39_Actor .


    ### ###########################################
    ### webern:Person

    webern-person:E21_Person rdf:type owl:Class ;

            kb:resourceIcon "default.png" ;

            prov:wasDerivedFrom ecrm:E21_Person ;

            rdfs:comment    """Diese Klasse umfasst reale Personen, die leben oder von denen angenommen wird, dass sie gelebt haben."""@de ,
                            """This class comprises real persons who live or are assumed to have lived."""@en ,
                            """Cette classe comprend les personnes réelles qui vivent ou sont présumées avoir vécu."""@fr ,
                            """Questa classe comprende le persone reali che vivono o si suppone che abbiano vissuto."""@it ;

            rdfs:isDefinedBy <http://www.knora.org/ontology/08AE/webern-person> ;

            rdfs:label  "E21 Person"@de ,
                        "E21 Person"@en ,
                        "E21 Personne"@fr ,
                        "E21 Persona"@it ;

            rdfs:subClassOf webern-person:E39_Actor ,  ecrm:E21_Person ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty webern-person:hasFamilyName ;
                              owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                              salsah-gui:guiOrder "0"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty webern-person:hasGivenName ;
                              owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                              salsah-gui:guiOrder "1"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty webern-person:hasAlternativeName ;
                              owl:minCardinality "0"^^xsd:nonNegativeInteger ;
                              salsah-gui:guiOrder "2"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty webern-person:hasIAFIdentifier ;
                              owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                              salsah-gui:guiOrder "3"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty webern-person:hasBirthPlace ;
                              owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                              salsah-gui:guiOrder "4"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty webern-person:hasBirthDate ;
                              owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                              salsah-gui:guiOrder "5"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty webern-person:hasDeathDate ;
                              owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                              salsah-gui:guiOrder "6"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty webern-person:hasDeathPlace ;
                              owl:maxCardinality "1"^^xsd:nonNegativeInteger ;
                              salsah-gui:guiOrder "7"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty webern-person:hasBiography ;
                              owl:minCardinality "0"^^xsd:nonNegativeInteger ;
                              salsah-gui:guiOrder "8"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty webern-person:hasBiographySource ;
                              owl:minCardinality "0"^^xsd:nonNegativeInteger ;
                              salsah-gui:guiOrder "9"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty webern-person:hasRelation ;
                              owl:minCardinality "0"^^xsd:nonNegativeInteger ;
                              salsah-gui:guiOrder "10"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty webern-person:comment ;
                              owl:minCardinality "0"^^xsd:nonNegativeInteger ;
                              salsah-gui:guiOrder "11"^^xsd:nonNegativeInteger
                            ] .
