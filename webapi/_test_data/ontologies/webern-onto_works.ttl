    ### webern:P102_hasTitle

    :P102_hasTitle rdf:type owl:ObjectProperty ;

              kb:subjectClassConstraint :F22_SelfContainedExpression ;  #TODO: E71 Man-Made THing

              kb:objectClassConstraint kb:TextValue ;       # TODO: eigentlich Verweis auf E35 Titel Objekt

              rdfs:subPropertyOf kb:hasValue, ecrm:P102_has_title ;

              rdfs:label "hat Titel"@de ,
                         "has title"@en ,
