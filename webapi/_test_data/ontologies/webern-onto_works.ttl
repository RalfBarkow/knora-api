    ### ###########################################
    ### webern:P3_hasNote

    :P3_hasNote rdf:type owl:ObjectProperty ;

            kb:subjectClassConstraint :E1_Entity ;

            kb:objectClassConstraint kb:TextValue ;

            prov:wasDerivedFrom ecrm:P3_has_note ;

            rdfs:comment    """Diese Property ist ein Container für alle informellen Beschreibungen zu einem Objekt, die nicht in Form von CRM-Konstrukten ausgedrückt wurden."""@de ,
                            """This property is a container for all informal descriptions about an object that have not been expressed in terms of CRM constructs."""@en ,
                            """Cette propriété est un conteneur pour toutes les descriptions informelles d'un objet qui n'ont pas été exprimées en termes de construction CRM."""@fr ,
                            """Questa proprietà è un contenitore per tutte le descrizioni informali di un oggetto che non sono state espresse in termini di costrutti CRM."""@it ;

            rdfs:isDefinedBy <http://www.knora.org/ontology/08AE/webern> ;

            rdfs:label  "P3 hat Anmerkung"@de ,
                        "P3 has note"@en ,
                        "P3 a une note"@fr ,
                        "P3 ha nota"@it ;

            rdfs:subPropertyOf kb:hasValue, ecrm:P3_has_note ;

            salsah-gui:guiElement salsah-gui:SimpleText .


    ### ###########################################
    ### webern:P4_hasTimeSpan

    :P4_hasTimeSpan rdf:type owl:ObjectProperty ;

            kb:subjectClassConstraint :E2_TemporalEntity ;

            kb:objectClassConstraint :E52_TimeSpan ;

            prov:wasDerivedFrom ecrm:P4_has_time-span ;

            rdfs:comment    """Diese Property beschreibt die zeitliche Einschränkung einer Instanz von E2 Temporal Entity."""@de ,
                            """This property describes the temporal confinement of an instance of an E2 Temporal Entity."""@en ,
                            """Cette propriété décrit le confinement temporel d'une instance d'une entité temporelle E2."""@fr ,
                            """Questa proprietà descrive il confinamento temporale di un'istanza di un'Entità Temporale E2."""@it ;

            rdfs:isDefinedBy <http://www.knora.org/ontology/08AE/webern> ;

            rdfs:label  "P4 hat Zeitspanne"@de ,
                        "P4 has time-span"@en ,
                        "P4 a une période de temps"@fr ,
                        "P4 ha un intervallo di tempo"@it ;

            rdfs:subPropertyOf kb:hasLinkTo, ecrm:P4_has_time-span ;

            salsah-gui:guiElement salsah-gui:Searchbox .


    :P4_hasTimeSpanValue rdf:type owl:ObjectProperty ;

            kb:subjectClassConstraint :E2_TemporalEntity ;

            kb:objectClassConstraint kb:LinkValue ;

            prov:wasDerivedFrom ecrm:P4_has_time-span ;

            rdfs:comment    """Diese Property beschreibt die zeitliche Einschränkung einer Instanz von E2 Temporal Entity."""@de ,
                            """This property describes the temporal confinement of an instance of an E2 Temporal Entity."""@en ,
                            """Cette propriété décrit le confinement temporel d'une instance d'une entité temporelle E2."""@fr ,
                            """Questa proprietà descrive il confinamento temporale di un'istanza di un'Entità Temporale E2."""@it ;

            rdfs:isDefinedBy <http://www.knora.org/ontology/08AE/webern> ;

            rdfs:label  "P4 hat Zeitspanne (value)"@de ,
                        "P4 has time-span (value)"@en ,
                        "P4 a une période de temps (value)"@fr ,
                        "P4 ha un intervallo di tempo (value)"@it ;

            rdfs:subPropertyOf kb:hasLinkToValue .

