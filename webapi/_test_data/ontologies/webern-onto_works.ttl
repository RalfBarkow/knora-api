    ### ###########################################
    ### webern:P148_hasComponent

    :P148_hasComponent rdf:type owl:ObjectProperty ;

            kb:subjectClassConstraint :E89_PropositionalObject ;

            kb:objectClassConstraint :E89_PropositionalObject ;

            rdfs:subPropertyOf kb:hasLinkTo, ecrm:P148_has_component ;

            rdfs:label  "hat Bestandteil"@de ,
                        "has component"@en ,
                        "a une composante"@fr ,
                        "ha componente"@it ;

            rdfs:comment    """Diese Property verbindet eine Instanz von E89 Propositional Object mit einem strukturellen Teil davon, der selbst eine Instanz von E89 Propositional Object ist."""@de ,
                            """This property associates an instance of E89 Propositional Object with a structural part of it that is by itself an instance of E89 Propositional Object."""@en ,
                            """Cette propriété associe une instance de l'objet propositionnel E89 à une partie structurelle de celui-ci qui est en soi une instance de l'objet propositionnel E89."""@fr ,
                            """Questa proprietà associa un'istanza di E89 Oggetto Propositivo con una parte strutturale di esso che è di per sé un'istanza di E89 Oggetto Propositivo."""@it ;

            salsah-gui:guiElement salsah-gui:Searchbox .

